<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>ServletStub</display-name>
    <servlet>
        <servlet-name>ServletStub</servlet-name>
        <servlet-class>robin.jetty.ServletStub</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletStub</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>

    <resource-ref>
        <res-ref-name>mail/SmtpServer</res-ref-name>
        <res-type>javax.mail.Session</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
</web-app>
